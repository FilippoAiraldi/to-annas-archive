[
    {
        "name": "IEEEXplore",
        "url": "https://ieeexplore.ieee.org/",
        "urlmatch": "ieeexplore",
        "extractors": {
            "title": "xplGlobal.document.metadata['title']",
            "authors": "xplGlobal.document.metadata[\"authors\"].map(author => author.name).join(', ')",
            "doi": "xplGlobal.document.metadata['doi']"
        }
    },
    {
        "name": "Oxford Academic",
        "url": "https://academic.oup.com/",
        "urlmatch": "academic.oup",
        "extractors": {
            "title": "dataLayer[0].full_title",
            "authors": "dataLayer[0].authors",
            "doi": "dataLayer[0].doi"
        }
    },
    {
        "name": "Springer Link",
        "url": "https://link.springer.com/",
        "urlmatch": "springer",
        "extractors": {
            "title": "document.querySelector('h1.c-article-title').textContent",
            "authors": "Array.from(document.querySelectorAll(\"a[data-test='author-name']\")).map(item => item.textContent).join(', ')",
            "doi": "dataLayer[0]['DOI']"
        }
    },
    {
        "name": "ScienceDirect",
        "url": "https://www.sciencedirect.com/",
        "urlmatch": "sciencedirect",
        "extractors": {
            "title": "document.querySelector('span.title-text').textContent",
            "authors": "Array.from(document.getElementsByClassName('text surname')).map(item => item.textContent).join(', ')",
            "doi": "document.querySelector('a.anchor.doi.anchor-primary').querySelector('span.anchor-text').textContent.slice(16)"
        }
    },
    {
        "name": "PubMed",
        "url": "https://pubmed.ncbi.nlm.nih.gov/",
        "urlmatch": "pubmed",
        "extractors": {
            "title": "document.querySelector('h1.heading-title').textContent.trim()",
            "authors": "Array.from(document.querySelectorAll('.authors-list-item')).map(item => item.firstChild.textContent).join(', ')",
            "doi": "document.querySelector('.doi').children[1].textContent.trim()"
        }
    },
    {
        "name": "JSTOR",
        "url": "https://www.jstor.org/",
        "urlmatch": "jstor",
        "extractors": {
            "title": "dataLayer[0].itemTitle",
            "authors": "Array.from(document.querySelectorAll('mfe-content-details-pharos-link[data-qa=\"item-authors\"]')).map(item => item.textContent).join(\", \")"
        }
    }
]
